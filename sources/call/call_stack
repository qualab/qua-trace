/// Declaration of internal class of call stack constant iterator data

#pragma once

#include <trace/call>
#include <deque>

namespace trace
{
    typedef std::deque<call> call_storage;

    class call::stack::const_iterator::data
    {
    public:
        typedef call_storage::const_iterator value_type;

        data(value_type value);

        value_type get_value() const;
        value_type& get_reference();

    private:
        value_type m_value;
    };
}

/// @author Владимир Керимов
